mg :src=\"getImg(tile.img)\" :alt=\"tile.title\">\n            </v-avatar>\n          </v-list-tile-avatar>\n          <v-list-tile-title style=\"color: black;\">{{ tile.title }}</v-list-tile-title>\n        </v-list-tile>\n      </v-list>\n    </v-bottom-sheet>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport isMobile from '../../../assets/js/isMobile.js';\nexport default {\n  props: {\n    config: null,\n    option_0: null,\n    option_1: null,\n    option_2: null\n  },\n  data: () => ({\n    display: null,\n    isMobile: null\n  }),\n  methods: {\n    action(index, params) {\n      this.$emit(`option_${index}`, params);\n      this.showSheet = false;\n    },\n    getImg(img) {\n      return require(`../../../assets/img/${img}`);\n    }\n  },\n  mounted() {\n    this.isMobile = isMobile;\n  },\n\n  computed: {\n    ...mapGetters({\n      getBottomModalState: 'getBottomModalState'\n    })\n  }\n};\n</script>\n\n<style>\n.onDesktop {\n  margin-left: 6% !important;\n}\n\n.noDesktop {\n  margin-left: 0 !important;\n}\n</style>\n"]}]}